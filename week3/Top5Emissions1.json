{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 300,
  "height": 200,
  "background": null,
  "data": {
    "url": "https://raw.githubusercontent.com/JamesOldham11/JamesOldham11.github.io/main/week3/Top5Emissions.csv"
  },
  "mark": "area",
  "encoding": {
    "x": {"field": "Date", "type": "temporal"},
    "y": {
      "aggregate": "sum",
      "field": "Billions",
      "type": "quantitative"
    },
    "color": {"field": "Country", "scale": {"scheme": "category20b"}}
  },
  "config": {"legend": {
  "orient": "bottom"}}
}
