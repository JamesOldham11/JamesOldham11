{
  "width": 290,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/JamesOldham11/JamesOldham11.github.io/main/Project/SolarCostDemand.csv"
  },
  "encoding": {"x": {"type": "temporal", "field": "Year"}},
  "layer": [
    {
      "mark": {"type": "line", "color": "red"},
      "encoding": {
        "y": {
          "aggregate": "average",
          "field": "Cost",
          "scale": {"domain": [0, 5]},
          "title": "Cost ($ per W)"
        }
      }
    },
    {
      "mark": {"type": "bar", "width": {"band": 0.5}, "opacity": 0.5},
      "encoding": {
        "y": {
          "aggregate": "average",
          "field": "GWH",
          "title": "GWh Generated UK",
          "scale": {"domain": [0, 15000]}
        }
      }
    }
  ],
  "resolve": {"scale": {"y": "independent"}},
  "config": {}
}