{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 100,
  "height": 100,
  "background": null,
  "data": {
    "url": "https://raw.githubusercontent.com/JamesOldham11/JamesOldham11.github.io/main/Project/TrellisData.csv"
  },
  "mark": "line",
  "encoding": {
    "facet": {"field": "Series", "type": "ordinal", "columns": 4, 
    "title": "Price Index (2015 = 100) of Gas/Electricity for Domestic Market vs other goods"},
    "x": {"field": "Date", "type": "temporal"},
    "y": {
      "field": "Value",
      "type": "quantitative",
      "scale": {"domain": [50, 300]},
      "title": "Price Index, 2015 = 100"
    },
    "tooltip": [
      {"field": "Value", "title": "Price Index"}
    ]
  },
  "config": {}
}
